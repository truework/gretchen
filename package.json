{
  "name": "gretchen",
  "version": "1.2.0",
  "description": "Making fetch happen in Typescript.",
  "source": "index.ts",
  "main": "dist/gretchen.js",
  "modern": "dist/gretchen.modern.js",
  "module": "dist/gretchen.esm.js",
  "unpkg": "dist/gretchen.iife.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
