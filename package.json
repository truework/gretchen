    "got",
    "axios",
    "node-fetch",
    "typescript",
    "tiny",
    "small",
    "micro",
    "microjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/truework/gretchen.git"
  },
  "author": "truework",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/truework/gretchen/issues"
  },
  "homepage": "https://github.com/truework/gretchen#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "microbundle": "^0.11.0",
    "node-fetch": "^2.6.0",
    "ts-node": "^8.4.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
