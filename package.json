{
  "name": "gretchen",
  "version": "1.2.0",
  "description": "Making fetch happen in Typescript.",
  "source": "index.ts",
  "main": "dist/gretchen.js",
  "module": "dist/gretchen.es.js",
  "unpkg": "dist/gretchen.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle build",
    "watch": "microbundle watch",
    "test": "ava tests/**.test.ts --verbose",
    "format": "prettier-standard --format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier-standard --format --staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "keywords": [
    "fetch",
    "fetching",
    "http",
    "https",
    "request",
    "requests",
    "get",
    "url",
    "ajax",
    "api",
    "got",
    "axios",
    "node-fetch",
    "typescript",
    "tiny",
    "small",
    "micro",
    "microjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/truework/gretchen.git"
  },
  "author": "truework",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/truework/gretchen/issues"
  },
  "homepage": "https://github.com/truework/gretchen#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "commitizen": "^4.2.2",
    "core-js": "^3.7.0",
    "cross-fetch": "^3.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.2.3",
    "microbundle": "^0.11.0",
    "prettier-standard": "^16.4.1",
    "semantic-release": "^17.2.2",
    "ts-node": "^8.4.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
